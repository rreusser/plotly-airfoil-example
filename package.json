{
  "name": "plotly-airfoil",
  "version": "1.0.0",
  "description": "A Karman-Trefftz airfoil using forthcoming plotly.js features",
  "main": "index.js",
  "scripts": {
    "watch": "nodemon -w airfoil.py --exec \"python airfoil.py\"",
    "start": "plotly-mock-viewer --mock-dir=./ --plotly-dir=../plotly.js",
    "build:html": "echo '' | indexhtmlify | htmlinjectscript bundle.js | metadataify | github-cornerify > index.html"
  },
  "keywords": [],
  "author": "Ricky Reusser",
  "license": "ISC",
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^14.3.0",
    "html-inject-script": "^1.1.0",
    "indexhtmlify": "^1.3.1",
    "metadataify": "^1.0.3",
    "plotly-mock-viewer": "^1.0.11"
  },
  "github-corner": {
    "url": "https://github.com/rreusser/plotly-airfoil-example"
  }
}
